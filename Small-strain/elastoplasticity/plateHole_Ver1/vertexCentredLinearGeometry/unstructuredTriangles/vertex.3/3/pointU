FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "3";
    object      pointU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    down
    {
        type            calculated;
    }
    right
    {
        type            calculated;
    }
    up
    {
        type            calculated;
    }
    left
    {
        type            calculated;
    }
    hole
    {
        type            calculated;
    }
}


