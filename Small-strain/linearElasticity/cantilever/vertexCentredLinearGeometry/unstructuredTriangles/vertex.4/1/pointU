FoamFile
{
    version     2.0;
    format      binary;
    class       pointVectorField;
    arch        "LSB;label=32;scalar=64";
    location    "1";
    object      pointU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    left
    {
        type            calculated;
    }
    bottom
    {
        type            calculated;
    }
    right
    {
        type            calculated;
    }
    top
    {
        type            calculated;
    }
}


