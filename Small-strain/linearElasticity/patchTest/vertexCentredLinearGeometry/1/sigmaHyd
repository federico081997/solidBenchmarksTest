FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      sigmaHyd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   nonuniform List<scalar> 5(-1.33333e+06 -1.33333e+06 -1.33333e+06 -1.33333e+06 -1.33333e+06);

boundaryField
{
    boundary
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


